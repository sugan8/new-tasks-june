public class Vowel{
    public static void main(String arg[]){
        String name="suganshanmugam";
        int count =0;
        
        for (char c:name.toLowerCase().toCharArray())
        {
            if(c=='a'||c=='e'||c=='i'||c=='o'||c=='u'){
                count++;
            }
        }
        System.out.println("vowels in the string is:"+count);
    }
}
