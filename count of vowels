// find how many vowels in this string"gokul"
public class Main{
    public static void main (String args[]){
        String a ="gokul";
        int count =0;
        
        for (int i=0;i<a.length();i++){
            char ch=a.charAt(i);
            if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'){
                count++;
            }
        }
        System.out.println("vowels in the string:"+count);
    }
}
