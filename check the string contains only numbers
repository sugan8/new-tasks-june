// check the string contains only integers
public class Main{
    public static void main(String args[]){
     String a=("12345");
     boolean isNumeric =true;
     for(char ch:a.toCharArray()){
         if(!Character.isDigit(ch)){
         isNumeric =false;
         break;
        }
    }
        if (isNumeric){
            System.out.println("it contains only numbers");
        }else{
            System.out.println("it not contains only numbers");
        }
        }
}
