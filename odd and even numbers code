// Print even and odd numbers separately from 1 to 50.
public class Main{
    public static void main(String args[]){
        System.out.println("even numbers from 1 to 50");
        for (int i=1;i<=50;i++){
            if (i%2==0){
                System.out.println(i+"");
            }
        }
        System.out.println("\n\nOdd numbers from 1 to 50:");
        for (int i=1;i<=50;i++){
        if (i % 2 != 0) {
            System.out.println(i+"");
          }
        }
    }
}
