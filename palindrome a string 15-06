public class Main{
    public static void main(String args[]){
        String name="sus";
        String b="";
        
        for(int i=name.length()-1;i>=0;i--){
            b+=name.charAt(i);
        }
        if (name.equals(b)){
            System.out.println("it is a palindrome");
        }else{
            System.out.println("It is not a palindrome");
        }
    }
}
